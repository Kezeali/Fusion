<!-- Cursor document, used by the Ogre3D sample. -->
<rml>
	<head>
		<title>Move</title>
		<style>
			body
			{
				width: 16px;
				height: 16px;
			}
			div.cursor
			{
				width: 16px;
				height: 16px;
				position: absolute;
				top: -8px;
				left: -8px;
				
				cursor-decorator: image;
				cursor-image: cursor-move.png;
			}
		</style>
	</head>
	<body>
	<div class="cursor"/>
	</body>
</rml>
