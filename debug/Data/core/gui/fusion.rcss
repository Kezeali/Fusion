{
	display: inline-block;

	width: 159px;
	height: 32px;

	padding-top: 8px;
	vertical-align: -10px;

	font-size: 18;
	text-align: center;
	tab-index: auto;

	embed-font-effect: shadow;
	embed-offset: 0px 1px;
	embed-color: white;

	background-decorator: image;
	background-image: fusion.png 252px 6px 399px 38px;
	background-z-index: 0;
}

button:focus,
input.submit:focus
{
	font-weight: bold;
	/*embed-offset: 0px 2px;*/
	color: #011B2F;
}

button:hover,
input.submit:hover
{
	background-image-t: 50px 82px;
}

button:active,
input.submit:active
{
	background-image-t: 96px 128px;

	color: white;

	embed-font-effect: none;
	shadow-font-effect: shadow;
	shadow-offset: 0px 1px;
	shadow-color: black;
}



