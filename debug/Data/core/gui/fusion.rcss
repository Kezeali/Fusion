body
{
	font-family: Delicious;
	font-weight: normal;
	font-style: normal;
	font-size: 15;
	color: black;
}

body.window
{
	padding-top: 20px;
	padding-bottom: 0px;
	
	min-width: 200px;
	max-width: 1000px;
	
	min-height: 80px;
	max-height: 700px;
}


div#title_bar
{
	z-index: 1;

	position: absolute;
	top: 0px;
	left: 0px;

	width: auto;
	height: 20px;
	
	text-align: left;

	background-decorator: tiled-horizontal;
	background-left-image: fusion.png 0px 120px 1px 140px;
	background-center-image: fusion.png repeat-truncate 2px 120px 18px 140px;
	background-right-image: fusion.png 161px 120px 162px 140px;
}

body.window:focus div#title_bar
{
	z-index: 1;

	position: absolute;
	top: 0px;
	left: 0px;

	width: auto;
	height: 20px;

	text-align: left;

	background-decorator: tiled-horizontal;
	background-left-image-t: 157px 177px;
	background-center-image-t: 157px 177px;
	background-right-image-t: 157px 177px;
}

div.rounded#title_bar
{
	z-index: 1;

	position: absolute;
	top: 0px;
	left: 0px;

	width: auto;
	height: 20px;
	
	text-align: left;

	background-decorator: tiled-horizontal;
	background-left-image: fusion.png 0px 0px 3px 30px;
	background-center-image: fusion.png repeat-truncate 4px 0px 20px 30px;
	background-right-image: fusion.png 160px 0px 163px 30px;
}

body.window:focus div.rounded#title_bar
{
	z-index: 1;

	position: absolute;
	top: 0px;
	left: 0px;

	width: auto;
	height: 20px;

	text-align: left;

	background-decorator: tiled-horizontal;
	background-left-image-t: 60px 90px;
	background-center-image-t: 60px 90px;
	background-right-image-t: 60px 90px;
}

div#title_bar div#icon
{
	position: absolute;
	left: 2px;
	top: 2px;
	
	width: 24px;
	height: 24px;
}

div#title_bar div#title
{
	font-size: 20;
	font-weight: bold;

	width: auto;
	height: 20px;

	color: white;

	outline-font-effect: outline;
	outline-width: 1px;
	outline-color: black;

	margin: 1px 10px 3px 24px;
}


div.close_button
{
	display: none;

	z-index: 2;

	position: absolute;
	top: 2px;
	right: 4px;

	width: 32px;
	height: 20px;
	icon-decorator: image;
	icon-image: fusion.png 204px 5px 236px 25px;
}

div.close_button:hover
{
	icon-image-t: 26px 46px;
}

div.close_button:active
{
	icon-image-t: 47px 67px;
}


div#window
{
	width: auto;
	padding: 0px 3px 0px 2px;

	background-decorator: tiled-box;
	background-top-left-image: fusion.png 0px 140px 1px 152px;
	background-top-image: fusion.png repeat-truncate 2px 140px 18px 152px;
	background-top-right-image: fusion.png 161px 140px 162px 152px;

	background-left-image: fusion.png stretch 0px 153px 1px 154px;
	background-center-image: fusion.png stretch 2px 153px 18px 154px;
	background-right-image: fusion.png stretch 161px 153px 162px 154px;

	background-bottom-left-image: fusion.png 0px 155px 1px 156px;
	background-bottom-image: fusion.png stretch 2px 155px 18px 156px;
	background-bottom-right-image: fusion.png 161px 155px 162px 156px;
}

body.window:focus div#window
{
	background-top-left-image-t: 176px 190px;
	background-top-image-t: 176px 190px;
	background-top-right-image-t: 176px 190px;

	background-left-image-t: 190px 191px;
	background-center-image-t: 190px 191px;
	background-right-image-t: 190px 191px;

	background-bottom-left-image-t: 192px 193px;
	background-bottom-image-t: 192px 193px;
	background-bottom-right-image-t: 192px 193px;
}

div#content
{
	z-index: 0;

	padding: 3px 0px 2px;

	width: auto;
	height: 100%;

	overflow: hidden auto;

	text-align: center;

	/*content-font-effect: shadow;
	content-offset: 1px 1px;
	content-color: white;*/
}

div#size_target
{
	
	width: 16px;
	height: 16px;
	icon-decorator: image;
	icon-image: fusion.png 58px 403px 76px 421px;
}



