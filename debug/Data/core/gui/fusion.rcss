/* Force the tabset element to a fixed size. */
tabset
{
	display: block;
	width: 280px;
	height: 200px;
}

/* Display the tab container as a block element 20 pixels high; it will
   be positioned at the top of the tabset. */
tabset tabs
{
	display: block;
	height: 31px;
	
	color: black;
}

/* Force each tab to only take up 50 pixels across the tabs element. */
tabset tab
{
	width: 50px;
	padding: 2px 8px 2px;
	text-align: center;
	
	background-decorator: tiled-horizontal;
	background-left-image: fusion.png 162px 192px 176px 223px;
	background-center-image: fusion.png stretch 176px 192px 177px 223px;
}

tabset tab:selected
{
	width: 50px;
	height: 20px;
	padding: 3px 8px 8px;
	background-left-image-t: 230px 267px;
	background-center-image-t: 230px 267px;
}

/* Display the panel container as a block element 180 pixels high; it will
   be positioned below the tab container and take up the rest of the space
   in the tabset. */
tabset panels
{
	display: block;
	height: 180px;
	
	padding: 10px 15px;

	background-decorator: tiled-box;
	background-top-left-image: fusion.png 0px 0px 133px 140px;
	background-top-right-image: fusion.png 136px 0px 146px 140px;
	background-top-image: fusion.png stretch 134px 0px 135px 140px;
	background-bottom-left-image: fusion.png 0px 140px 11px 151px;
	background-bottom-right-image: fusion.png 136px 140px 146px 151px;
	background-bottom-image: fusion.png stretch 11px 140px 12px 151px;
	background-left-image: fusion.png stretch 0px 139px 10px 140px;
	background-center-image: fusion.png stretch 11px 139px 12px 140px;
}

/* Fix each panel to take up exactly the panelled space. */
tabset panels panel
{
	display: block;
	width: 100%;
	height: 100%;
}
